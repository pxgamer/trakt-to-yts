{
    "name": "pxgamer/trakt-to-yts",
    "type": "library",
    "description": "A CLI for downloading Trakt lists from YTS.",
    "license": "MIT",
    "bin": [
        "trakt-to-yts"
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-zero/framework": "^6.0"
    },
    "require-dev": {
        "humbug/box": "^3.2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
